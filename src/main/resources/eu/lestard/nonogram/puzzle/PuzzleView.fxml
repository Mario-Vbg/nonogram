<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.lestard.nonogram.puzzle.PuzzleView">
   <center>

    <AnchorPane fx:id="rootPane" prefHeight="600.0" prefWidth="600.0">
         <children>
              <AnchorPane fx:id="overviewPane" />
              <AnchorPane fx:id="topNumberPane" />
              <AnchorPane fx:id="leftNumberPane" />
              <AnchorPane fx:id="centerPane" />
         </children>
    </AnchorPane>

   </center>
   <top>
      <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
            <HBox fx:id="errorsBox" spacing="2.0" />
        </items>
      </ToolBar>
   </top>
</BorderPane>
